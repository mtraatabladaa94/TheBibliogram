<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup useLegacyV2RuntimeActivationPolicy="true">
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ILibroServicio" />
                <binding name="BasicHttpBinding_IIdiomaServicio" />
                <binding name="BasicHttpBinding_IGradoServicio" />
                <binding name="BasicHttpBinding_IEncabezadoServicio" />
                <binding name="BasicHttpBinding_IEditorServicio" />
                <binding name="BasicHttpBinding_IInstitucionServicio" />
                <binding name="BasicHttpBinding_IPrestamoServicio" />
                <binding name="BasicHttpBinding_IInventarioServicio" />
                <binding name="BasicHttpBinding_IPersonaServicio" />
                <binding name="BasicHttpBinding_ICarreraServicio" />
                <binding name="BasicHttpBinding_ICiudadServicio" />
                <binding name="BasicHttpBinding_IPaisServicio" />
                <binding name="BasicHttpBinding_IAccountServicio" />
                <binding name="NoSecurity" maxReceivedMessageSize="2147483647">
                    <readerQuotas maxDepth="32" maxStringContentLength="8388608"
                        maxArrayLength="2147483647" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
                    <security mode="None" />
                </binding>
                <binding name="BasicHttpBinding_IRecomendacionServicio" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:5587/Catalogacion/LibroServicio.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILibroServicio"
                contract="LibroServicio.ILibroServicio" name="BasicHttpBinding_ILibroServicio" />
