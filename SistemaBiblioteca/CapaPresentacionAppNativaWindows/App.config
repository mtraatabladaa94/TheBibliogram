                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IEncabezadoServicio"
                contract="EncabezadoServicio.IEncabezadoServicio" name="BasicHttpBinding_IEncabezadoServicio" />
            <endpoint address="http://localhost:5587/Catalogacion/EditorServicio.svc"
